{
	"name": "snipper",
	"displayName": "Snipper",
	"description": "Snipper is that allows you to import code snippets from a public github repository.",
	"version": "0.0.1",
	"publisher": "alexis walter",
	"repository": "https://github.com/arzeo68/snippet-manager",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./extension/out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.list",
				"title": "Snipper"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc --inlineSources true -p ./ && npm run postbuild",
		"postbuild": "cp package.json extension/out/package.json && cd extension/out && npm install --only=production",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@babel/core": "^7.23.2",
		"@babel/preset-env": "^7.23.2",
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"babel-loader": "^9.1.3",
		"eslint": "^8.26.0",
		"ts-loader": "^9.5.0",
		"typescript": "^5.2.2",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"run": "^1.4.0",
		"watch": "^1.0.2"
	},
	"dependencies": {
		"axios": "^1.6.0"
	}
}
